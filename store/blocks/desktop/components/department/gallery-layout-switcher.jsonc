{
    "flex-layout.row#global__galleryswitcher--search": {
        "title": "Flex row Gallery Layout Switcher",
        "children": [
            "gallery-layout-switcher#global__galleryswitcher--search",
            "order-by.v2"
        ],
        "props": {
            "horizontalAlign": "right",
            "fullWidth": true,
            "blockClass": "global__galleryswitcher--search"
        }
    },
    "gallery-layout-switcher#global__galleryswitcher--search": {
        "children": [
            "gallery-layout-option#grid",
            "gallery-layout-option#list"
        ],
        "props": {
            "blockClass": "global__galleryswitcher--search"
        }
    },
    "gallery-layout-option#grid": {
        "props": {
            "name": "grid"
        },
        "children": [
            "image#icon__grid-switcher"
        ]
    },
    "gallery-layout-option#list": {
        "props": {
            "name": "list"
        },
        "children": [
            "image#icon__list-switcher"
        ]
    },
    "image#icon__grid-switcher": {
        "title": "Image Icon Grid",
        "props": {
            "maxWidth": "5",
            "maxHeight": "5",
            "src": "data:image/png;base64,",
            "blockClass": "icon__grid-switcher"
        }
    },
    "image#icon__list-switcher": {
        "title": "Image Icon List",
        "props": {
            "maxWidth": "5",
            "maxHeight": "5",
            "src": "https://www.creativefabrica.com/wp-content/uploads/2019/04/List-icon-by-ahlangraphic-2.jpg",
            "blockClass": "icon__list-switcher"
        }
    }
}